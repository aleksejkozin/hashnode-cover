{
  "name": "hashnode-cover",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=14"
  },
  "comments": {
    "warning": "Any package.json update will cause a huge Docker recompile. Try to avoid updating this file",
    "dependencies": "There is no difference between devDependencies and dependencies, both will be in production",
    "dev": "Will watch for all files and run all services. Needs .env.global to function",
    "loadenv": "Will create .env.global, should not be run with a nx runner cuz it will not wait for async to finish",
    "deploy": "Will push everything to the cloud"
  },
  "scripts": {
    "dev": "yarn loadenv && nx run-many --target=serve --parallel --all",
    "loadenv": "nx build loadenv --prod && env-cmd node dist/packages/loadenv/main.js",
    "deploy": "nx run-many --target=deploy --all --prod"
  },
